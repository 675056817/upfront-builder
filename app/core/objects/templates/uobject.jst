  {[ } ]}
  {[ if (elementSpecificProperties) { ]}
  {{ elementSpecificProperties }}
  {[ } ]}
  'options' => array(
    'theme_style' => '{{ theme_style }}',
    'anchor' => '{{ anchor }}',
    {{ elementOptions }}
	)
));
